
<%if(success && success.length){%>
    <div class="alert alert-success alert-dismissible fade show my-5 display-5" role="alert">
        <%=success%>
    <button type="button" class="btn-close fs-3 " data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%>

<%if(error && error.length){%>
    <div class="alert alert-danger alert-dismissible fade show my-5 display-5" role="alert">
        <%=error%>
    <button type="button" class="btn-close fs-3" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<%}%>